<hr>

Bilar: <%= allcars.length %>
<%

for (var i = 0; i < allcars.length; i++) { %>
  <form data-edit="true" action="/cars" method="post">
    <input type="hidden" name="_method" value="delete"/>
    <input type="hidden" name="id" value="<%= allcars[i]._id %>">
    <select name="typ">
      <option value="volvo" <% if (allcars[i].typ == 'volvo') { %> selected <% } %>>Volvo</option>
      <option value="saab" <% if (allcars[i].typ == 'saab') { %> selected <% } %>>Saab</option>
      <option value="mercedes" <% if (allcars[i].typ == 'mercedes') { %> selected <% } %>>Mercedes</option>
      <option value="audi" <% if (allcars[i].typ == 'audi') { %> selected <% } %>>Audi</option>
    </select>
    <br>
    Automat
    <select name="automat">
      <option value="true">Ja</option>
      <option value="false" <% if (allcars[i].automat == false) { %> selected <% } %>>Nej</option>
    </select>
    <br>
    Takräcke
    <select name="rail">
      <option value="true">Ja</option>
      <option value="false" <% if (allcars[i].rail == false) { %> selected <% } %>>Nej</option>
    </select>
    <br>
    <input type="text" name="price" placeholder="price" value="<%= allcars[i].price %>"><br>
    <br>
    Bokad
    <select name="booked">
      <option value="true">Ja</option>
      <option value="false" <% if (allcars[i].booked == false) { %> selected <% } %>>Nej</option>
    </select>
    <br>
    <input type="submit" value="PATCH">
    <br>
    <input type="submit" value="DELETE">
  </form>
  <% }
  %>

<script src="/js/jquery-3.1.1.min.js" type="text/javascript"></script>
<script src="/js/main.js" type="text/javascript"></script>
